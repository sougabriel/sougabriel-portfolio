{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "sougabriel-portfolio": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "changeDetection": "OnPush",
                    "displayBlock": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/sougabriel-portfolio",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": ["zone.js", "@angular/localize/init"],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true,
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            },
                            "src/robots.txt",
                            "src/sitemap.xml"
                        ],
                        "styles": [
                            "@angular/material/prebuilt-themes/azure-blue.css",
                            "src/styles.scss",
                            {
                                "input": "src/styles/material-icons.scss",
                                "inject": true
                            },
                            {
                                "input": "src/styles/custom-themes/azure-blue.scss",
                                "bundleName": "azure-blue",
                                "inject": false
                            },
                            {
                                "input": "src/styles/custom-themes/cyan-orange.scss",
                                "bundleName": "cyan-orange",
                                "inject": false
                            },
                            {
                                "input": "src/styles/custom-themes/magenta-violet.scss",
                                "bundleName": "magenta-violet",
                                "inject": false
                            },
                            {
                                "input": "src/styles/custom-themes/rose-red.scss",
                                "bundleName": "rose-red",
                                "inject": false
                            }
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kB",
                                    "maximumError": "10kB"
                                }
                            ],

                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "serviceWorker": "ngsw-config.json"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "sougabriel-portfolio:build:production"
                        },
                        "development": {
                            "buildTarget": "sougabriel-portfolio:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing",
                            "@angular/localize/init"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            }
                        ],
                        "styles": [
                            "@angular/material/prebuilt-themes/azure-blue.css",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}
